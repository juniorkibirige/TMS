{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "flutter",
			"command": "flutter",
			"args": [
				"pub",
				"run",
				"build_runner",
				"watch",
				"--delete-conflicting-outputs",
				"--use-polling-watcher"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": "build",
			"label": "Generate G Files",
			"presentation": {
				"panel": "dedicated"
			}
		}
	]
}